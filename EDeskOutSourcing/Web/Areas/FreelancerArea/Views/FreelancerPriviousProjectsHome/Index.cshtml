@model IEnumerable<FreelancerPriviousProjects>

<div class="card">
    <div class="card-body">
        <h5 class="card-title">Prvious Projects List</h5>

        <a class="btn btn-success" asp-action="Create" asp-controller="FreelancerPriviousProjectsHome"> Create </a>
        <hr />
            <!-- Bordered Table -->
            <table class="table table-bordered table-responsive">
                <thead>
                    <tr>
                        <th scope="col">#</th>
                        <th scope="col">Project Title</th>
                        <th scope="col">Project Description</th>
                        <th scope="col">Actions</th>
                    </tr>
                </thead>
                <tbody>
                    @{
                        int srno = 1;
                    }
                    @foreach (var temp in Model)
                    {
                        <tr>
                            <th scope="row">@srno</th>
                            <td>@temp.ProjectTitle</td>
                            <td>@temp.ProjectDescription</td>
                            <td>
                                <a class="btn btn-outline-primary" asp-action="Edit" asp-route-id="@temp.FreelancerPriviousProjectsId">Edit</a>
                                |
                                <a class="btn btn-outline-primary" asp-action="Delete" asp-route-id="@temp.FreelancerPriviousProjectsId">Delete</a>
                            </td>
                        </tr>
                        srno++;
                    }
                </tbody>
            </table>
            <!-- End Bordered Table -->


    </div>
</div>