@model IEnumerable<ProjectStage>

<div class="card-body">
    <h5 class="card-title">Project Stages</h5>
    <hr />
    @foreach (var temp in Model)
    {
        <div class="row">
            <div class="col-3"><label class="control-label">@temp.ProjectStageName</label></div>
            <div class="col-9">: @temp.StageDescription</div>
        </div>
    }
    
</div>