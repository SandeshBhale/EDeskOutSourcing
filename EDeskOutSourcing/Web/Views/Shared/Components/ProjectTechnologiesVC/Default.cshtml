@model IEnumerable<ProjectTechnology>

<div class="card-body">
    <h5 class="card-title">Project Technologies</h5>
    <hr />
    @foreach (var temp in Model)
    {
        <div class="row">
            <div class="col-3"><label class="control-label">@temp.Technologies.TechnologyName</label></div>
        </div>
    }
    
</div>